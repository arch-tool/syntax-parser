{
  "name": "cparser",
  "version": "0.1.0",
  "pri": {
    "type": "component"
  },
  "scripts": {
    "start": "pri dev",
    "docs": "pri docs",
    "build": "pri build",
    "preview": "pri preview",
    "analyse": "pri analyse",
    "test": "pri test",
    "format": "tslint --fix './src/**/*.?(ts|tsx)' && prettier --write './src/**/*.?(ts|tsx)'",
    "prepublishOnly": "npm run build",
    "bundle": "pri bundle",
    "asbuild:untouched": "asc assembly/index.ts -b build/untouched.wasm -t build/untouched.wat --sourceMap --validate",
    "asbuild:optimized": "asc assembly/index.ts -b build/optimized.wasm -t build/optimized.wat --sourceMap --validate --optimize --noDebug",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized"
  },
  "devDependencies": {
    "pri": "^0.12.1"
  },
  "main": "dist/src/index.js",
  "types": "src/index.tsx",
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0"
  }
}
