                     END AS 编辑器ka编辑器
                    ,CASE    WHEN t.编辑器='编辑器' AND t.编辑器='编辑器' AND t.编辑器ka='编辑器ka'  AND t.编辑器=ds  THEN t.编辑器 
                     END AS 编辑器ka编辑器
                    ,CASE    WHEN t.编辑器='编辑器' AND t.编辑器='编辑器' AND t.编辑器ka='编辑器ka'  AND t.编辑器=ds  THEN t.编辑器 
                     END AS 编辑器ka编辑器
                    ,CASE    WHEN t.编辑器='编辑器' AND t.编辑器='编辑器' AND t.编辑器ka='编辑器ka'  AND t.编辑器=ds  THEN t.编辑器 
                     END AS 编辑器ka编辑器
                    ,CASE    WHEN t.编辑器='编辑器' AND t.CASE编辑器='编辑器case' AND t.编辑器ka='编辑器ka'  AND t.编辑器=ds   THEN t.case_id 
                     END AS 编辑器ka编辑器CASE编辑器
                    ,CASE    WHEN t.编辑器='编辑器' AND t.CASE编辑器='编辑器case' AND t.编辑器ka='编辑器ka'  AND t.编辑器=ds   THEN t.case_id 
                     END AS 编辑器ka编辑器CASE编辑器
                    ,CASE    WHEN t.编辑器='编辑器' AND t.CASE编辑器='编辑器case' AND t.编辑器ka='编辑器ka'  AND t.编辑器=ds   THEN t.case_id 
                     END AS 编辑器ka编辑器CASE编辑器
                    ,CASE    WHEN t.编辑器='编辑器' AND t.CASE编辑器='编辑器case' AND t.编辑器ka='编辑器ka'  AND t.编辑器=ds   THEN t.case_id 
                     END AS 编辑器ka编辑器CASE编辑器
                    ,CASE    WHEN t.编辑器='编辑器' AND t.CASE编辑器='编辑器case' AND t.编辑器ka='编辑器ka'  AND t.编辑器=ds   THEN t.编辑器 
                     END AS 编辑器ka编辑器
                    ,CASE    WHEN t.编辑器='编辑器' AND t.CASE编辑器='编辑器case' AND t.编辑器ka='编辑器ka'  AND t.编辑器=ds   THEN t.编辑器 
                     END AS 编辑器ka编辑器
                    ,CASE    WHEN t.编辑器='编辑器' AND t.CASE编辑器='编辑器case' AND t.编辑器ka='编辑器ka'  AND t.编辑器=ds   THEN t.编辑器 
                     END AS 编辑器ka编辑器
                    ,CASE    WHEN t.编辑器='编辑器' AND t.CASE编辑器='编辑器case' AND t.编辑器ka='编辑器ka'  AND t.编辑器=ds   THEN t.编辑器 
                     END AS 编辑器ka编辑器
                    ,CASE    WHEN t.编辑器='编辑器' AND t.CASE编辑器='编辑器case' AND t.编辑器='编辑器' AND t.编辑器ka='编辑器ka' AND  t.编辑器=ds   THEN t.编辑器 
                     END AS 编辑器ka编辑器
                    ,CASE    WHEN t.编辑器='编辑器' AND t.CASE编辑器='编辑器case' AND t.编辑器='编辑器' AND t.编辑器ka='编辑器ka' AND  t.编辑器=ds   THEN t.编辑器 
                     END AS 编辑器ka编辑器
                    ,CASE    WHEN t.编辑器='编辑器' AND t.CASE编辑器='编辑器case' AND t.编辑器='编辑器' AND t.编辑器ka='编辑器ka' AND  t.编辑器=ds   THEN t.编辑器 
                     END AS 编辑器ka编辑器
                    ,CASE    WHEN t.编辑器='编辑器' AND t.CASE编辑器='编辑器case' AND t.编辑器='编辑器' AND t.编辑器ka='编辑器ka' AND  t.编辑器=ds   THEN t.编辑器 
                     END AS 编辑器ka编辑器
            FROM    (
                        SELECT  ds
                               ,a.case_id
                                ,a.编辑器 AS 编辑器
                                ,CASE    WHEN d.ka_type like '%KA%' THEN 'ka' 
                                         ELSE '编辑器ka' 
                                 END AS 编辑器ka
                                ,TO_CHAR(create_time,'yyyymmdd') AS 编辑器
                                ,TO_CHAR(case_end_time,'yyyymmdd') AS 编辑器
                                ,a.CASE编辑器
                                ,a.编辑器
                                ,b.object_id AS 编辑器
                                ,b.编辑器
                                ,b.编辑器
                                --,t1.编辑器caseid
                                ,CASE    WHEN c.exe_status=0 THEN '编辑器'
                                         WHEN c.exe_status=1 THEN '编辑器'
                                         WHEN c.exe_status=2 THEN '编辑器' 
                                 END AS 编辑器
                        FROM    (
                                    SELECT  ds
                                            ,case_id
                                            ,member_name
                                            ,member_id
                                            ,create_time
                                            ,case_end_time
                                            ,GET_JSON_OBJECT(ext_attrs, '$.scenarioName') AS 编辑器
                                            ,CASE    WHEN is_qc_case='Y' THEN '编辑器case'
                                                     WHEN ext_attrs LIKE '%isReboot%' THEN '编辑器case'
                                                     WHEN ext_attrs LIKE '%rebootCaseId%' THEN '编辑器case' 
                                                     ELSE '编辑器case' 
                                             END AS CASE编辑器
                                            -- ,GET_JSON_OBJECT(ext_attrs, '$.rebootCaseId') AS 编辑器caseid
                                            ,CASE    WHEN member_request_type=1 THEN '编辑器'
                                                     WHEN member_request_type=2 THEN '编辑器'
                                                     WHEN member_request_type=3 THEN '编辑器'
                                                     WHEN member_request_type=4 THEN '编辑器' 
                                             END AS 编辑器
                                    FROM    crm_cdm.dwd_tb_crm_qiankun_case_df
                                    WHERE   ds >='201800101'
                                    --AND     TO_CHAR(create_time,'yyyymmdd') = '20180826'
                                    AND     case_type = '701'
                                    AND member_request_type IN ('1','2','3','4')
                                ) a
                       left outer join(
                                    SELECT  case_id
                                            ,object_id
                                            ,form_id
                                            ,GET_JSON_OBJECT(handle_ext_attrs,'$.matchRule') AS 编辑器
                                            ,CASE    WHEN GET_JSON_OBJECT(handle_ext_attrs,'$.actionIds') IS NOT NULL THEN '编辑器' 
                                                     ELSE '编辑器' 
                                             END AS 编辑器
                                    FROM    crm_cdm.dwd_tb_crm_qiankun_unit_df
                                    WHERE   ds = MAX_PT('crm_cdm.dwd_tb_crm_qiankun_unit_df')
                                    AND     case_type = '701'
                                    AND     SUBSTR(handle_create_time,1,10) >= '2018-01-01'
                                    AND     member_request_type IN (1,2,3,4)
                                ) b
                        ON      a.case_id = b.case_id
                        LEFT OUTER JOIN (
                                            SELECT  form_id
                                                    ,exe_status
                                            FROM    crm_cdm.dwd_tb_crm_qiankun_result_action_df
                                            WHERE   ds = MAX_PT('crm_cdm.dwd_tb_crm_qiankun_result_action_df')
                                            AND     SUBSTR(action_create_time,1,10) >= '2018-01-01'
                                        ) c
                        ON      b.form_id = c.form_id
                        LEFT OUTER JOIN (
                                            SELECT  user_id
                                                    ,ka_type
                                            --,ds
                                            FROM    cco_busi.dim_tb_gm_ka_all
                                            WHERE   ds = MAX_PT('cco_busi.dim_tb_gm_ka_all')
                                        ) d
                        ON      a.member_id = d.user_id
                    ) t
            GROUP BY t.ds
                     ,t.编辑器ka
                     ,t.编辑器
                     ,t.编辑器
                     ,t.CASE编辑器
                     ,t.编辑器
                     ,t.编辑器
                     ,t.编辑器
                     ,t.编辑器
                     ,t.case_id
        ) t2
GROUP BY t2.ds